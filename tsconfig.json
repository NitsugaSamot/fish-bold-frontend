{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "lib": [
      "dom",
      "es2018"
    ],
    "allowJs": true,
    "checkJs": false,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "incremental": true,
    "resolveJsonModule": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
, "pages/%5Btoken%5D.js"  ],
  "exclude": [
    "node_modules"
  ]
}
